{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "extensions-cli": {
            "projectType": "application",
            "schematics": {},
            "root": "",
            "sourceRoot": "src",
            "prefix": "",
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "",
                        "main": "src/__extensions_index.ts",
                        "tsConfig": "tsconfig.app.json",
                        "aot": false,
                        "customWebpackConfig": {
                            "path": "./build/webpack-custom.ts",
                            "mergeStrategies": {
                                "externals": "replace"
                            }
                        }
                    },
                    "configurations": {
                        "prod": {
                            "optimization": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "extensions-cli:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "extensions-cli:build:production"
                        }
                    }
                }
            }
        }
    },
    "defaultProject": "extensions-cli"
}
